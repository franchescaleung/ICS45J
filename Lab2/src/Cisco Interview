class Node:
	this.head = none
	this.next = none
	this.value = 0
	


// put into queue
def enqueue(node, integer):
	if node.head == none:
		node.value = integer
	else:
		node.next.value = integer
		
// first item out
def dequeue(node):
	elementToReturn = node.value
	node.head = node.next
	return elementToReturn

	// get max
def max(node):
	max = 0;
	content = node.head // pointer to node
	// set variable to node
	// while node is not none, if node is greater than max, replace max
	
	while content != none:
		if content.value > max:
			max = content.value
		content = content.next
		
	return max
	